{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",

  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "http://34.228.112.95/api/$1"
    },
    {
      "source": "/media/(.*)",
      "destination": "http://34.228.112.95/media/$1"
    },
    {
      "source": "/static/(.*)",
      "destination": "http://34.228.112.95/static/$1"
    },
    {
      "source": "/((?!.*\\.(?:js|css|png|jpg|jpeg|gif|svg|ico|woff2?|ttf|map|json)).*)",
      "destination": "/index.html"
    }
  ]
}
